<?xml version="1.0" encoding="utf-8"?>
<shortcuts xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:ignore="MissingDefaultResource">
    <shortcut
        android:shortcutId="search"
        android:enabled="true"
        android:icon="@drawable/shortcut_search"
        android:shortcutShortLabel="@string/shortcut_search">
        <intent
            android:action="android.intent.action.VIEW"
            android:targetPackage="com.fsh.android.wanandroidmvvm"
            android:targetClass="com.fsh.android.wanandroidmvvm.module.search.view.SearchActivity" />
        <categories android:name="android.shortcut.conversation" />
    </shortcut>
    <shortcut
        android:shortcutId="collect"
        android:enabled="true"
        android:icon="@drawable/shortcut_wenda"
        android:shortcutShortLabel="@string/shortcut_wenda">
        <intent
            android:action="android.intent.action.VIEW"
            android:targetPackage="com.fsh.android.wanandroidmvvm"
            android:targetClass="com.fsh.android.wanandroidmvvm.module.question.view.QuestionArticleListActivity" />
        <categories android:name="android.shortcut.conversation" />
    </shortcut>

</shortcuts>